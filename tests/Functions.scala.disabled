package tests

import project.Operators.LogicalOp,LogicalOp.*
import project.Function.{semtab, Tableau}

object Semtab extends TestFramework:
    def exampleFromProjectStatement: Result[Tableau] =
        val formula: List[LogicalOp] = List(
            Or(Atom("p"), Atom("q")),
            Atom("r"),
            Atom("q")
        )

        val output = List(
            List(0, 0, 1),
            List(0, 1, 1),
            List(1, 0, 0),
            List(1, 1, 0),
            List(1, 0, 1),
            List(1, 1, 1),
            List(1, 0, 0),
            List(1, 1, 0)
        )

        this.assertEqual(Some(semtab(formula)), Some(output))